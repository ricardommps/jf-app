{
  "cli": {
    "version": ">= 7.8.6"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "dev",
        "EXPO_PUBLIC_API_URL": "http://192.168.15.59:8001",
        "EXPO_PUBLIC_IS_DEV": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_ENV": "dev",
        "EXPO_PUBLIC_API_URL": "http://192.168.15.59:8001",
        "EXPO_PUBLIC_IS_DEV": "true"
      }
    },
    "production": {
      "distribution": "store",
      "ios": {
        "simulator": false
      },
      "env": {
        "APP_ENV": "prod",
        "EXPO_PUBLIC_API_URL": "https://jf-app-api.vercel.app",
        "EXPO_PUBLIC_IS_DEV": "false"
      }
    },
    "test": {
      "env": {
        "APP_ENV": "dev",
        "EXPO_PUBLIC_API_URL": "https://jf-app-api.vercel.app",
        "EXPO_PUBLIC_IS_DEV": "true"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
