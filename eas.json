{
  "cli": {
    "version": ">= 7.8.6"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "dev",
        "EXPO_PUBLIC_API_URL": "http://192.168.15.59:8001",
        "EXPO_PUBLIC_IS_DEV": "true",
        "GOOGLE_SERVICES_JSON": "file:google-services.json"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_ENV": "dev",
        "EXPO_PUBLIC_API_URL": "http://192.168.15.59:8001",
        "EXPO_PUBLIC_IS_DEV": "true",
        "GOOGLE_SERVICES_JSON": "file:google-services.json"
      }
    },
    "production": {
      "distribution": "store",
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "APP_ENV": "prod",
        "EXPO_PUBLIC_API_URL": "https://jf-app-api.vercel.app",
        "EXPO_PUBLIC_IS_DEV": "false",
        "GOOGLE_SERVICES_JSON": "file:google-services.json"
      }
    },
    "test": {
      "env": {
        "APP_ENV": "dev",
        "EXPO_PUBLIC_API_URL": "https://jf-app-api.vercel.app",
        "EXPO_PUBLIC_IS_DEV": "true",
        "GOOGLE_SERVICES_JSON": "file:google-services.json"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "ricardommps@gmail.com",
        "ascAppId": "6750486916"
      },
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "internal"
      }
    }
  }
}
