{
  "expo": {
    "name": "FOLTZ",
    "slug": "jfapp",
    "owner": "jftreinos",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/jf_icone_app.png",
    "scheme": "jfapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": ["**/*"],
    "linking": {
      "prefixes": [
        "jfapp://",
        "https://connect.garmin.com",
        "https://www.strava.com"
      ]
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.jftreinos.jfapp",
      "buildNumber": "4",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Precisamos acessar suas fotos para alterar o avatar.",
        "NSCameraUsageDescription": "Precisamos acessar a câmera para tirar uma foto.",
        "NSPhotoLibraryAddUsageDescription": "Precisamos salvar fotos no seu dispositivo.",
        "UIBackgroundModes": ["remote-notification"],
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "jfapp",
            "CFBundleURLSchemes": ["jfapp"]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:connect.garmin.com",
        "applinks:www.strava.com"
      ],
      "runtimeVersion": "1.0.1",
      "deploymentTarget": "15.1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/jf_icone_app.png",
        "backgroundColor": "#2b2b2b"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.jftreinos.jfapp",
      "googleServicesFile": "./google-services.json",
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "buildToolsVersion": "34.0.0",
      "minSdkVersion": 21,
      "versionCode": 4,
      "intentFilters": [
        {
          "action": "android.intent.action.VIEW",
          "category": [
            "android.intent.category.DEFAULT",
            "android.intent.category.BROWSABLE"
          ],
          "data": { "scheme": "jfapp" }
        },
        {
          "action": "android.intent.action.VIEW",
          "category": [
            "android.intent.category.DEFAULT",
            "android.intent.category.BROWSABLE"
          ],
          "data": {
            "scheme": "https",
            "host": "connect.garmin.com",
            "pathPrefix": "/modern/activity"
          }
        },
        {
          "action": "android.intent.action.VIEW",
          "category": [
            "android.intent.category.DEFAULT",
            "android.intent.category.BROWSABLE"
          ],
          "data": {
            "scheme": "https",
            "host": "www.strava.com",
            "pathPrefix": "/activities"
          }
        }
      ],
      "permissions": ["android.permission.RECORD_AUDIO"],
      "runtimeVersion": "1.0.1"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/jf_icone_app.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#2b2b2b"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/jf_logo_icone_push.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": true,
          "sounds": []
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "A aplicação precisa acessar suas fotos para alterar a foto do perfil.",
          "cameraPermission": "A aplicação precisa acessar a câmera para tirar fotos do perfil."
        }
      ],
      "./plugins/receive-sharing-intent.js",
      "./plugins/fix-firebase-pods.js",
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "6ddb8b5c-3423-408e-b94b-f7dd9b7b14f7"
      }
    },
    "notification": {
      "icon": "./assets/images/jf_logo_icone_push.png",
      "color": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/6ddb8b5c-3423-408e-b94b-f7dd9b7b14f7"
    }
  }
}
